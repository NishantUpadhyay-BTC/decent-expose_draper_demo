<%= form_for user do |f|%>

<%= f.label :city %>
<%= f.text_field :city %>
<br>
<%= f.label :state %>
<%= f.text_field :state %>
<br>
<%= f.label :country %>
<%= f.text_field :country %>
<br>
<%= f.label :email %>
<%= f.text_field :email %>
<br>
<%= f.label :username %>
<%= f.text_field :username %>
<br>
<%= f.label :full_name%>
<%= f.text_field :full_name %>
<br>
<%= f.label :twitter_name %>
<%= f.text_field :twitter_name %>
<br>
<%= f.label :github_name %>
<%= f.text_field :github_name %>
<br>
<%= f.label :website_url %>
<%= f.text_field :website_url%>
<br>
<%= f.label :phone_number %>
<%= f.text_field :phone_number %>
<br>
<%= f.label :birth_date %>
<%= f.date_select :birth_date %>
<br>
<%= f.submit 'Submit' %>
<%end%>